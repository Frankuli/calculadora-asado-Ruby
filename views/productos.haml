!!!
%html.no-js{:lang => "zxx"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    / Mobile Specific Meta
    %meta{:content => "width=device-width, initial-scale=1, shrink-to-fit=no", :name => "viewport"}/
    / Favicon
    %link{:href => "img/fav.png", :rel => "shortcut icon"}/
    / Author Meta
    %meta{:content => "Colorlib", :name => "author"}/
    / Meta Description
    %meta{:content => "", :name => "description"}/
    / Meta Keyword
    %meta{:content => "", :name => "keywords"}/
    / meta character set
    %meta{:charset => "UTF-8"}/
    / Site Title
    %title CellOn
    %link{:href => "https://fonts.googleapis.com/css?family=Poppins:100,300,500", :rel => "stylesheet"}/
    /
      CSS
      \=============================================
    %link{:href => "css/linearicons.css", :rel => "stylesheet"}/
    %link{:href => "css/owl.carousel.css", :rel => "stylesheet"}/
    %link{:href => "css/font-awesome.min.css", :rel => "stylesheet"}/
    %link{:href => "css/nice-select.css", :rel => "stylesheet"}/
    %link{:href => "css/magnific-popup.css", :rel => "stylesheet"}/
    %link{:href => "css/bootstrap.css", :rel => "stylesheet"}/
    %link{:href => "css/main.css", :rel => "stylesheet"}/
  %body
    .oz-body-wrap
      / Start Header Area
      %header.default-header
        .container-fluid
          .header-wrap
            .header-top.d-flex.justify-content-between.align-items-center
              .logo
                %a{:href => "index.html"}
                  %img{:alt => "", :src => "img/logo.png"}/
              .main-menubar.d-flex.align-items-center
                %nav.hide
                  %a{:href => "index.html"} Home
                  %a{:href => "generic.html"} Producto
                  %a{:href => "elements.html"} Personas
                  %a{:href => "elements.html"} Evento
                .menu-bar
                  %span.lnr.lnr-menu
      / End Header Area
      %section.banner-area.relative
        .container
          .row.fullscreen.align-items-center.justify-content-center
            .section-top-border
              %h3.mb-30 Listados de productos
              .progress-table-wrap
                .progress-table
                  .table-head
                    .serial #
                    .country Producto
                    .visit
                    .percentage
                  .table-row
                    .serial 01
                    .country Lechuga
                    .visit
                      %a{:href => "#elimina"} Eliminar
                    .percentage
                      %a{:href => "#editar"} Editar
      / Start Conatct- Area
      %section.contact-area.pt-100.pb-100.relative
        .overlay.overlay-bg
        .container
          .row.justify-content-center.text-center
            .single-contact.col-lg-6.col-md-8
              %h2.text-white
                Agregar
                %span Nuevos productos
              %p.text-white
                Productos involucrados en el asadazo.
          %form#myForm.contact-form{:action => "mail.php", :method => "post"}
            .row.justify-content-center
              .col-lg-5
                %input.common-input.mt-20{:name => "fname", :onblur => "this.placeholder = 'Enter your name'", :onfocus => "this.placeholder = ''", :placeholder => "Ingrese nombre de producto", :required => "", :type => "text"}/
              .col-lg-10.d-flex.justify-content-end
                %button.primary-btn.white-bg.d-inline-flex.align-items-center.mt-20
                  %span.mr-10 Agregar
                  %span.lnr.lnr-arrow-right
                %br/
              .alert-msg
      / End Conatct- Area
      / Strat Footer Area
      %footer.section-gap
        .container
          .footer-bottom.d-flex.justify-content-between.align-items-center.flex-wrap
            / Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0.
            %p.footer-text.m-0
              Trabajo práctico de Programación 3 ©
              :javascript
                document.write(new Date().getFullYear());
              All rights reserved | This work is made with
              %i.fa.fa-heart-o{"aria-hidden" => "true"}
              by
              %a{:href => "https://colorlib.com", :target => "_blank"} El Grupo
            / Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0.
      / End Footer Area
    %script{:src => "js/vendor/jquery-2.2.4.min.js"}
    %script{:crossorigin => "anonymous", :integrity => "sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4", :src => "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"}
    %script{:src => "js/vendor/bootstrap.min.js"}
    %script{:src => "js/jquery.ajaxchimp.min.js"}
    %script{:src => "js/owl.carousel.min.js"}
    %script{:src => "js/jquery.nice-select.min.js"}
    %script{:src => "js/jquery.magnific-popup.min.js"}
    %script{:src => "js/jquery.counterup.min.js"}
    %script{:src => "js/waypoints.min.js"}
    %script{:src => "js/main.js"}
